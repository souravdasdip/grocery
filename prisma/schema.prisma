// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Product{
  id String @id @default(cuid())
  catSlug String
  createdAt DateTime @default(now())
  title String
  desc String
  img String
  price Decimal
}